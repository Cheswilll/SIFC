	<queryString language="SQL">
		<![CDATA[SELECT
     partidos.`idPartido` AS partidos_idPartido,
     partidos.`idTipoPartido` AS partidos_idTipoPartido,
     partidos.`noIdentificacionProfesor` AS partidos_noIdentificacionProfesor,
     partidos.`tiempoJuado` AS partidos_tiempoJuado,
     partidos.`amonestaciones` AS partidos_amonestaciones,
     partidos.`nombreEquipoRival` AS partidos_nombreEquipoRival,
     partidos.`horaDelPartido` AS partidos_horaDelPartido,
     partidos.`observaciones` AS partidos_observaciones,
     partidos.`fechaDelPartido` AS partidos_fechaDelPartido,
     partidos.`idTipoAlineacion` AS partidos_idTipoAlineacion,
     partidos.`golesAfavor` AS partidos_golesAfavor,
     partidos.`golesEnContra` AS partidos_golesEnContra,
     partidos.`lugarPartido` AS partidos_lugarPartido,
     partidos.`idTorneo` AS partidos_idTorneo,
     partidos.`direccion` AS partidos_direccion,
     torneos.`idTorneo` AS torneos_idTorneo,
     torneos.`idTipoTorneo` AS torneos_idTipoTorneo,
     torneos.`lugarTorneo` AS torneos_lugarTorneo,
     torneos.`duracion` AS torneos_duracion,
     tipostorneos.`idTipoTorneo` AS tipostorneos_idTipoTorneo,
     tipostorneos.`nombreTipoTorneo` AS tipostorneos_nombreTipoTorneo
FROM
     `torneos` torneos INNER JOIN `partidos` partidos ON torneos.`idTorneo` = partidos.`idTorneo`
     INNER JOIN `tipostorneos` tipostorneos ON torneos.`idTipoTorneo` = tipostorneos.`idTipoTorneo`
WHERE
     tipostorneos.idTipoTorneo = 1]]>
	</queryString>
